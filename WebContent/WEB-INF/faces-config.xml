<navigation-rule>
  <from-view-id>/jsp/login/login.jsp</from-view-id>
  <navigation-case>
   <from-outcome>NO_AUTH</from-outcome>
   <to-view-id>/jsp/login/login.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>AUTH</from-outcome>
   <to-view-id>/jsp/login/principal.jsp</to-view-id>
  </navigation-case>
</navigation-rule>
<navigation-rule>
  <from-view-id>/jsp/login/logout.jsp</from-view-id>
  <navigation-case>
   <from-outcome>LOGIN</from-outcome>
   <to-view-id>/jsp/login/login.jsp</to-view-id>
  </navigation-case>
</navigation-rule>
 
